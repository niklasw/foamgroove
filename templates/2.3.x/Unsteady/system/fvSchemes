FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// * * * Scheme macro section  * * * * * * * * * * * * * * * * * * * * * * * //

Correction                 corrected; //limited 0.33;

ConvectionModel            Gauss;

// * * * End scheme macro section  * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         backward;
    //default         localEuler rDeltaT;
}

gradSchemes
{
    default         Gauss linear;
    grad(p)         Gauss linear;
    grad(U)         cellLimited Gauss linear 1;
}

divSchemes
{
    default             $ConvectionModel limitedLinear 1 phi;
    div(phi,U)          $ConvectionModel LUST unlimitedGrad(U);
    div(phi,k)          $ConvectionModel linearUpwind grad(U);
    div(phi,epsilon)    $ConvectionModel limitedLinear 1;
    div(phi,omega)      $ConvectionModel limitedLinear 1;
    div(phi,nuTilda)    $ConvectionModel limitedLinear 1;

    div((nuEff*dev(T(grad(U))))) Gauss linear;
    div(R)              Gauss  linear;
}

laplacianSchemes
{
    default                 Gauss linear $Correction;
}

interpolationSchemes
{
    default         linear;
}

snGradSchemes
{
    default         $Correction;
}

fluxRequired
{
    default         no;
    p               ;
    pcorr;
    p_rgh;
}
#includeIfPresent "include/freeSurfaceFvSchemesI";

// ************************************************************************* //
