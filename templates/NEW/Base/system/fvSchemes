FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

Correction          corrected; //corrected 0.33;
ConvectionModel     Gauss;

ddtSchemes
{
    default         backward;
}

gradSchemes
{
    default         Gauss linear;
}

divSchemes
{
    defaul          Gauss linear;
}

laplacianSchemes
{
    default         Gauss linear $Correction;
}

interpolationSchemes
{
    default         linear;
}

snGradSchemes
{
    default         $Correction;
}

fluxRequired
{
    default         no;
}

