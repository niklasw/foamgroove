FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nuTilda;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include "${FOAM_CASE}/setup.foam";

dimensions      [ 0 2 -1 0 0 0 0 ];

internalField   $:turbulence.initialnuTilda;

boundaryField
{
    #include "${WM_PROJECT_DIR}/etc/caseDicts/setConstraintTypes";

    inflowGroup
    {
        type            fixedValue;
        value           $:turbulence.initialnuTilda;
    }

    wallGroup
    {
        type            fixedValue;
        value           uniform 0;
    }

    slipGroup
    {
        type slip;
    }

    movingWallGroup
    {
        $wallGroup;
    }

    outflowGroup
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           uniform 0;
    }

    ambienceGroup
    {
        $outflowGroup;
    }

    farfieldGroup
    {
        type freestream;
        freestreamValue $internalField;
    }
}

// ************************************************************************* //
