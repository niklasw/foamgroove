FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

Correction          corrected; //limited 0.33;
ConvectionModel     bounded Gauss;

ddtSchemes
{
    default         steadyState;
}

gradSchemes
{
    default         Gauss linear;
    grad(p)         Gauss linear;
    gradLimiter     cellLimited Gauss linear 1;
}

divSchemes
{
    //default             $ConvectionModel upwind;
    div(phi,U)          $ConvectionModel linearUpwindV gradLimiter;
    turbulence          $ConvectionModel linearUpwind gradLimiter;
    div(phi,k)          $turbulence;
    div(phi,epsilon)    $turbulence;
    div(phi,omega)      $turbulence;
    div(phi,nuTilda)    $turbulence;
    div(R)              Gauss linear;
    div((nuEff*dev(T(grad(U))))) Gauss linear;
}

laplacianSchemes
{
    default                 Gauss linear $Correction;
}

interpolationSchemes
{
    default         linear;
}

snGradSchemes
{
    default         $Correction;
}

// Required for version > 3.0
wallDist
{
        method          meshWave;
}

// Not required for version > 3.0
fluxRequired
{
    default         no;
    p;
    pcorr;
    p_rgh;
    Phi;
}

// ************************************************************************* //
