FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include "$FOAM_CASE/simpleSetup.in"

dimensions      [ 1 -1 -2 0 0 0 0 ];

internalField   uniform $ambientPressure;

boundaryField
{
    ".*"
    {
        type            zeroGradient;
    }
    "(.*[Oo]utlet.*|.*[Aa]tmospher.*)"
    {
        type            totalPressure;
        U               U;
        phi             phi;
        rho             none;
        psi             none;
        gamma           1;
        p0              $ambientPressure;
    }
    "(.*[Oo]pening.*|.*[Aa]mbien.*)"
    {
        type            totalPressure;
        U               U;
        phi             phi;
        rho             none;
        psi             none;
        gamma           1;
        p0              $ambientPressure;
    }

#   includeIfPresent    "include/freeStream.in"
#   include             "include/general.in"

}


// ************************************************************************* //
