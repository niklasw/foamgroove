FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// * * * Scheme macro section  * * * * * * * * * * * * * * * * * * * * * * * //

Correction          corrected; //limited 0.33;
divMethod           bounded Gauss;

// * * * End scheme macro section  * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         steadyState;
}

gradSchemes
{
    default         Gauss linear;
    grad(p)         Gauss linear;
    grad(U)         cellLimited Gauss linear 1;
    gradLimiter     cellLimited Gauss linear 1;
}

divSchemes
{
    default             $divMethod upwind;
    div(phi,U)          $divMethod linearUpwindV grad(U);
    turbulence          $divMethod linearUpwind gradLimiter;
    div(phi,k)          $turbulence;
    div(phi,epsilon)    $turbulence;
    div(phi,omega)      $turbulence;
    div(phi,nuTilda)    $turbulence;
    div(R)              Gauss linear;
    div((nuEff*dev(T(grad(U))))) Gauss linear;
    div((muEff*dev(T(grad(U))))) Gauss linear;
}

laplacianSchemes
{
    default                 Gauss linear $Correction;
}

interpolationSchemes
{
    default         linear;
}

snGradSchemes
{
    default         $Correction;
}

fluxRequired
{
    default         no;
    p               ;
}


// ************************************************************************* //
