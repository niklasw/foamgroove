FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nuTilda;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include "${FOAM_CASE}/simpleSetup.in"

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform $initialnuTilda;

boundaryField
{
    ".*"
    {
        type            zeroGradient;
    }
    ".*[Ii]nlet.*"
    {
        type            fixedValue;
        value           $internalField;
    }
    ".*[Ww]all.*"
    {
        type                fixedValue;
        value               uniform 0;
    }

#    include   "${FOAM_CASE}/system/commonPatches.in"
}


