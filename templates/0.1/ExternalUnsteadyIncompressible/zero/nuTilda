FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nuTilda;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include "../simpleSetup.in"

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    ".*"
    {
        type            zeroGradient;
    }

    ".*[Ii]nlet.*"
    {
        type            fixedValue;
        value           uniform 0;
    }
    ".*[Ww]all.*"
    {
        type                fixedValue;
        value               uniform 0;
    }
    ".*[Ss]ymmetry.*"
    {
        type            symmetryPlane;
    }
}


