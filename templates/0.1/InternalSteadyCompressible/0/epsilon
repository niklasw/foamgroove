FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include "${FOAM_CASE}/simpleSetup.in"

dimensions      [ 0 2 -3 0 0 0 0 ];

internalField   uniform $initialEps;

boundaryField
{
    ".*"
    {
        type            compressible::epsilonWallFunction;
        Cmu             0.09;
        kappa           0.41;
        E               9.8;
        value           $internalField;
    }
    "procBoundary.*"
    {
        type            processor;
        value           $internalField;
    }
    ".*[Ii]nlet.*"
    {
        type            compressible::turbulentMixingLengthDissipationRateInlet;
        mixingLength    $turbulentLengthScale;
        phi             phi;
        k               k;
        value           $internalField;
    }
    ".*[Oo]utlet.*"
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }
    ".*[Oo]pening.*"
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }
#    include   "${FOAM_CASE}/system/commonPatches.in"
}


