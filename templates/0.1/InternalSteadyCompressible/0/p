FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include "${FOAM_CASE}/simpleSetup.in"

dimensions      [ 1 -1 -2 0 0 0 0 ];

internalField   uniform $ambientPressure;

boundaryField
{
    ".*"
    {
        type            zeroGradient;
    }
    "procBoundary.*"
    {
        type            processor;
        value           $internalField;
    }
    ".*[Ii]nlet.*"
    {
        type            mixed;
        refValue        uniform $ambientPressure;
        refGradient     uniform 0;
        valueFraction   uniform 0.3;
    }
    ".*[Oo]utlet.*"
    {
        type            fixedValue;
        value           uniform $ambientPressure;
    }
    ".*[Oo]pening.*"
    {
        type            totalPressure;
        U               U;
        phi             phi;
        rho             none;
        psi             none;
        gamma           1;
        p0              uniform $ambientPressure;
    }
#    include   "${FOAM_CASE}/system/commonPatches.in"
}


// ************************************************************************* //
