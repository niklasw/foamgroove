FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include "$FOAM_CASE/simpleSetup.in"

dimensions      [ 0 1 -1 0 0 0 0 ];

internalField   uniform $initialVelocity;

boundaryField
{
    ".*"
    {
        type            fixedValue;
        value           uniform ( 0 0 0 );
    }
    ".*[Ii]nlet.*"
    {
        type            flowRateInletVelocity;
        flowRate        $flowrate;
        value           $internalField;
    }
    ".*[Oo]utlet.*"
    {
        type            zeroGradient;
    }
    ".*[Oo]pening.*"
    {
        type            pressureInletOutletVelocity;
        value           uniform (0 0 0);
    }
#   include "$FOAM_CASE/system/commonPatches.in"
}


