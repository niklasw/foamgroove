FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

convertToMeters 0.001;

vertices
(
{%- for v in vertices %}
    {{ v }}
{%- endfor %}
);

blocks
(
{%- for b in blocks %}
    {{ b }}
{%- endfor %}
);

edges
(
);
boundary
(
{%- for p in patches %}
    {{ p[0] }}
    {
        type {{ p[1] }};
        faces
        (
           {%- for f in p[2] %}
                {{ f }}
           {%- endfor %}
        );
    }
{%- endfor %}
);

mergePatchPairs
(
);

