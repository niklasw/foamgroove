#%Module1.0#####################################################################
##
## ansa/ansa modulefile
##
proc ModulesHelp { } {
}

prereq openfoam

module-whatis   "Adds paths to OpenFOAM templates"

set version             0.1
set user                $env(USER)
set prefix              /home/${user}/OpenFOAM/${user}-2.1.x/foamGroove

prepend-path    PATH            "${prefix}/bin"
setenv  OpenFOAM_TEMPLATES_DIR  "${prefix}/templates"
